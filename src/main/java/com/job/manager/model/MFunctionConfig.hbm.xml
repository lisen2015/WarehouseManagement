<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.job.manager.model.MFunctionConfig" table="M_FUNCTION_CONFIG" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="ID">
            <generator class="uuid"/>
        </id>
        <property name="channelId" column="CHANNEL_ID"/>
        <property name="functionName" column="FUNCTION_NAME"/>
        <property name="functionIndex" column="FUNCTION_INDEX"/>
        <property name="htmlUrl" column="HTML_URL"/>
        <property name="htmlId" column="HTML_ID"/>
        <property name="isValidate" column="IS_VALIDATE"/>
        <property name="inputTime" column="INPUT_TIME"/>
        <property name="updateTime" column="UPDATE_TIME"/>
        <property name="deleteTime" column="DELETE_TIME"/>
        <property name="createUser" column="CREATE_USER"/>
        <property name="lastUpdateUser" column="LAST_UPDATE_USER"/>
    </class>
</hibernate-mapping>