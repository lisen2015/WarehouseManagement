<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.job.manager.model.VInInventory" table="V_ININVENTORY" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="ID">
            <generator class="uuid"/>
        </id>
        <property name="productId" column="PRODUCTID"/>
        <property name="productName" column="PRODUCT_NAME"/>
        <property name="supplier" column="SUPPLIER"/>
        <property name="price" column="PRICE"/>
        <property name="inNumber" column="INNUMBER"/>
        <property name="inputTime" column="INPUT_TIME"/>
        <property name="createUserName" column="CREATE_USERNAME"/>
    </class>
</hibernate-mapping>