<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.job.manager.model.VProduct" table="V_PRODUCT" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="ID">
            <generator class="uuid"/>
        </id>
        <property name="productName" column="PRODUCT_NAME"/>
        <property name="department" column="DEPARTMENT"/>
        <property name="departmentName" column="DEPARTMENTNAME"/>
        <property name="outinventory" column="OUTINVENTORY"/>
        <property name="allNumber" column="ALLNUMBER"/>
        <property name="price" column="PRICE"/>
        <property name="inputTime" column="INPUT_TIME"/>
        <property name="updateTime" column="UPDATE_TIME"/>
        <property name="createUser" column="CREATE_USER"/>
        <property name="lastUpdateUser" column="LAST_UPDATE_USER"/>
        <property name="lastUpdateUserName" column="LAST_UPDATE_USERNAME"/>
    </class>
</hibernate-mapping>