<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.job.manager.model.MDepartment" table="M_DEPARTMENT" dynamic-update="true" dynamic-insert="true">
        <id name="id" column="ID">
            <generator class="uuid"/>
        </id>
        <property name="departmentName" column="DEPARTMENTNAME"/>
        <property name="departmentDesc" column="DEPARTMENTDESC"/>
        <property name="inputTime" column="INPUT_TIME"/>
        <property name="updateTime" column="UPDATE_TIME"/>
        <property name="createUser" column="CREATE_USER"/>
        <property name="lastUpdateUser" column="LAST_UPDATE_USER"/>
    </class>
</hibernate-mapping>